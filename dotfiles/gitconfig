[user]
  name = Josh Cheek
  email = josh.cheek@gmail.com
[color]
  ui = true
[color "diff"]
  whitespace = red reverse
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  brpb = !git branch --no-color 2> /dev/null | ruby -ne 'print $_.sub(\"*\", \"\").strip if $_.start_with? \"*\"' | pbcopy
  shas = log --pretty=format:\"%H\"
  hist = log --pretty=format:\"%Cgreen%h%Creset %Cblue%ad%Creset %s%C(yellow)%d%Creset %Cblue[%an]%Creset\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  praise = blame
[push]
  default = upstream
[core]
  excludesfile = ~/.gitignore
