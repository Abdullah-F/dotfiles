#!/usr/bin/env ruby
require 'shellwords'
require 'json'

class BuildResult
  class GemInfo
    class Installation
      attr_accessor :name, :ruby, :version
      def initialize(name:, version:, ruby:)
        self.name    = name
        self.ruby    = ruby
        self.version = version
      end

      def self.from_json(json)
        new json
      end

      def as_json
        {name: name, version: version, ruby: ruby}
      end
    end


    attr_accessor :name, :installations
    def initialize(name:, installations:[])
      self.name          = name
      self.installations = installations
    end

    def add(version:, ruby:)
      installations << Installation.new(name: name, version: version, ruby: ruby)
    end

    def to_s
      "#{name} (#{installations.map(&:version).sort.reverse.join(', ')})"
    end

    def inspect
      versions = installations.map { |i| "#{i.version}:#{i.ruby}" }.join(' ')
      "#<GemInfo:#{name} #{versions}>"
    end

    def self.from_json(json)
      new name:          json[:name],
          installations: json[:installations].map { |i| Installation.from_json i }
    end

    def as_json
      { name: name, installations: installations.map(&:as_json) }
    end
  end

  attr_accessor :gem_infos, :rubies, :failures
  def initialize(gem_infos:{}, rubies:[], failures:[])
    self.gem_infos = gem_infos
    self.rubies    = rubies
    self.failures  = failures
  end

  def add_installation(name:, **gem_attrs)
    gem_infos[name] ||= GemInfo.new(name: name)
    gem_infos[name].add gem_attrs
  end

  def self.from_json(json)
    new rubies:   json[:rubies],
        failures: json[:failures].map { |failure| ExecResult.from_json failure },
        gem_infos: json[:gem_infos].each_with_object({}) { |info, infos|
          info = GemInfo.from_json info
          infos[info.name] = info
        }
  end

  def as_json
    { gem_infos: gem_infos.map { |name, gem_info| gem_info.as_json },
      rubies:    rubies,
      failures:  failures.map(&:as_json),
    }
  end

  def formatted_failures
    failures.join("\n")
  end

  def each_gem_info
    gem_infos.sort_by(&:first).each do |_name, gem_info|
      yield gem_info
    end
  end
end

ExecResult = Struct.new :description, :exitstatus, :stdout, :stderr, :command, :spawn_command do
  def initialize(description:, exitstatus:, stdout:, stderr:, command:, spawn_command:)
    super description, exitstatus, stdout, stderr, command, spawn_command
  end

  def failure?
    !success?
  end

  def success?
    exitstatus.zero?
  end

  def self.from_json(json)
    new description:   json[:description],
        exitstatus:    json[:exitstatus],
        stdout:        json[:stdout],
        stderr:        json[:stderr],
        command:       json[:command],
        spawn_command: json[:spawn_command]
  end

  def as_json
    { description:   description,
      exitstatus:    exitstatus,
      stdout:        stdout,
      stderr:        stderr,
      command:       command,
      spawn_command: spawn_command
    }
  end

  def to_s
    str = ""
    str   << "Failure: #{description}\n"
    str   << "  Exitstatus:    #{exitstatus}\n"
    str   << "  Command:       #{command.inspect}\n"
    str   << "  Spawn Command: #{spawn_command.inspect}\n"

    if stdout.lines.length < 2
      str << "  Stdout:        #{stdout.inspect}\n"
    else
      str << "  Stdout:\n"
      str << stdout.gsub(/^/, '    | ')
      str << "\n" unless str.end_with? "\n"
    end

    if stderr.lines.length < 2
      str << "  Stderr:        #{stderr.inspect}\n"
    else
      str << "  Stderr:\n"
      str << stderr.gsub(/^/, '    | ')
      str << "\n" unless str.end_with? "\n"
    end
    str
  end
end

def self.execute(description:, command:)
  spawn_command = [
    'bash',
    '-c',
    "source /usr/local/share/chruby/chruby.sh; #{command.shelljoin}"
  ]
  read_stdout, write_stdout = IO.pipe
  read_stderr, write_stderr = IO.pipe
  pid = spawn *spawn_command, in: :close, out: write_stdout, err: write_stderr
  write_stdout.close
  write_stderr.close
  ExecResult.new description:   description,
                 exitstatus:    Process.detach(pid).join.value.exitstatus,
                 stdout:        read_stdout.read,
                 stderr:        read_stderr.read,
                 command:       command,
                 spawn_command: spawn_command
ensure
  read_stdout.close unless read_stdout.closed?
  read_stderr.close unless read_stderr.closed?
end

def self.load_build
  json = JSON.parse DATA.read, symbolize_names: true
  BuildResult.from_json json
end

case ARGV.first
when 'build'
  build = BuildResult.new

  # find rubies
  chruby = execute description: 'Find rubies', command: ['chruby']
  raise "Chruby failed: #{chruby.inspect}" if chruby.failure?
  build.rubies = chruby.stdout.gsub(/^[ *]*/, "").lines.map(&:chomp)

  # find gems
  build.rubies.each do |ruby|
    gem_list = execute description: "Find gems in #{ruby.inspect}",
                       command:     ['chruby-exec', ruby, '--', 'ruby', '-S', 'gem', 'list']
    next build.failures << gem_list if gem_list.failure?
    gem_list.stdout.lines.each(&:chomp!).each do |gem_with_version|
      formatted_versions = gem_with_version.scan(/\(.*?\)/).last
      gem_name           = gem_with_version.chomp(formatted_versions).strip
      versions           = formatted_versions[1...-1].split(', ')
      versions.each do |version|
        build.add_installation name: gem_name, version: version, ruby: ruby
      end
    end
  end

  # list failures
  puts build.formatted_failures

  # save results
  File.open DATA, 'r+' do |file|
    file.seek DATA.pos
    file.puts build.as_json.to_json
  end
when 'failures'
  puts load_build.formatted_failures
when 'gems'
  build = load_build
  puts "Gems:"
  build.each_gem_info { |gem_info| puts gem_info.to_s.gsub(/^/, '  ') }
when 'rubies'
when 'install'
  build = load_build
  gem = build.find_gem name: ARGV[1], version: ARGV[2]
  # gem_list = execute description: "Find gems in #{ruby.inspect}",
  #                    command:     ['chruby-exec', ruby, '--', 'ruby', '-S', 'gem', 'list']
  # `ruby -e 'p Gem::Specification.all.first.version.to_s' ^/dev/null`
  # chruby-exec 2.1.2 -- ruby -e 'puts Gem::Dependency.new("parser", "=2.1.9").to_spec.cache_file' | xargs -J '{}' cp '{}' .
  # chruby-exec 2.1.2 -- ruby -e 'puts Gem::Dependency.new("parser", "=2.1.9").to_spec.cache_file; puts Gem::Dependency.new("slop", "=3.6.0").to_spec.cache_file ' | xargs -I '{}' cp '{}' .
  require "pry"
  binding.pry
when 'test'
when 'json'
  puts load_build.as_json.to_json
else
  # chruby-exec ruby-2.1.2 -- ruby -e 'p Gem::Specification.first.name'
  # ruby -r pp -e 'p Gem::Specification.all.first.cache_file'
  # ruby -r pp -e 'p Gem::Specification.all.first.version.version'
  #
  # list all gems for each ruby
  #   can we get this in a consumable way?
  #   can we get it to tell us where the gem is cached?
  #
  # ruby -r json -e 'puts File.read("out").lines.map { |line| JSON.load line.chomp }.inject([], :+).group_by { |h| h["name"] }.first'
end
__END__
